import java.util.Random;

public class PilasAleatorio {
	public static void main(String[] args) {
		int max = 9999;
		int min = 0;
		Random rand = new Random();
		int randomNum;
		for (int i = 0; i < 100; i++) {
			randomNum = rand.nextInt((max - min) + 1)+ min;
			System.out.println(randomNum);
		}
	}
}
