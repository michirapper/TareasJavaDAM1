
public class temperatura {
	double celsisusToFarenheit;
	double farenheitToCelsius;
	
	public void setEdad(double celsisusToFarenheit) {
		double grado = celsisusToFarenheit;
		F=1,8 * grado +32;
	}
	static double F = 1.8 + 32;
	static double C =(F-32)/1.8;
	public static void main(String[] args) {
		System.out.println("1,8 grados Farenheit son " + F +  " ");
		System.out.println("1,8 grados Celsius son " + C);

	}

}
